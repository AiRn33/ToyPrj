<input type="hidden" id="shopNumber" value="{{shopNumber}}">

<script type="text/javascript">

var val = document.getElementById('shopNumber').value;

var delConfirm = confirm('게시글을 삭제 하시겠습니까?');
   if (delConfirm) {
      alert('게시글을 삭제되었습니다');
      location.href='/shop/deleteShopProc/' + val;
   }
   else {
      alert('이전페이지로 돌아갑니다');
      location.href='javascript:history.back()';
   }
</script>